[Nemo Action]
Name=Convert to MP2
Comment=Convert video to MPEG2 with ffmpeg (for playback on DVD Players, TV's or Personal Media Devices)
EscapeSpaces=true
Exec=~/.local/share/powertoys/convert_video_to_video.sh "mp2-ask" %F
Selection=S
Extensions=mpg;mp4;mpeg;avi;m4v;mov;mkv;divx;xvid;wmv
Dependencies=ffmpeg;zenity